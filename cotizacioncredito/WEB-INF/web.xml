<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>Cotizacion</display-name>
	<servlet>
		<servlet-name>CotizacionServlet</servlet-name>
		<servlet-class>cotizacion.CotizacionServlet</servlet-class>
		<init-param>
			<param-name>base</param-name>
			<param-value>/cotizacioncredito/inicio</param-value>
		</init-param>
		<init-param>
			<param-name>jdbcDriver</param-name>
			<param-value>com.mysql.cj.jdbc.Driver</param-value>
		</init-param>
		<init-param>
			<param-name>dbURL</param-name>
			<param-value>jdbc:mysql://localhost:3306/socios</param-value>
		</init-param>
		<init-param>
			<param-name>dbUser</param-name>
			<param-value>NombreUsuario</param-value>
		</init-param>
		<init-param>
			<param-name>dbPass</param-name>
			<param-value>PassUsuario</param-value>
		</init-param>
	</servlet>
</web-app>
